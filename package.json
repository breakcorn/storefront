{
	"name": "saleor-storefront",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"license": "BSD-3-Clause",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "vite preview",
		"predev": "pnpm run generate",
		"prebuild": "pnpm run generate",
		"generate": "graphql-codegen --config .graphqlrc.ts",
		"prepare": "is-ci || husky install",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev:local": "dotenv -e .env.development -- pnpm run dev",
		"install:browsers": "pnpm exec playwright install",
		"dev:docker": "./deploy.sh",
		"build:development": "dotenv -e .env.development -- pnpm run build",
		"build:production": "cp svelte.config.local.js svelte.config.js && dotenv -e .env.production -- pnpm run build",
		"build:test": "dotenv -e .env.test -- pnpm run build",
		"build:local": "cp svelte.config.local.js svelte.config.js && pnpm run build",
		"test": "dotenv -e .env.test -- playwright test",
		"test:local": "BASE_URL=http://localhost:3000 dotenv -e .env.test -- playwright test",
		"test:docker": "./run-tests-docker.sh",
		"test:docker:full": "./test-docker.sh",
		"test:headed": "dotenv -e .env.test -- playwright test --headed",
		"test:debug": "dotenv -e .env.test -- playwright test --debug",
		"docker:deploy": "./deploy.sh",
		"docker:stop": "./stop.sh",
		"docker:logs": "./logs.sh",
		"docker:status": "./status.sh",
		"docker:dev": "./deploy.sh",
		"docker:prod": "NODE_ENV=production ./deploy.sh",
		"vercel:build": "pnpm run build",
		"vercel:dev": "vercel dev"
	},
	"dependencies": {
		"@adyen/adyen-web": "5.53.3",
		"@adyen/api-library": "15.0.0-beta",
		"@saleor/auth-sdk": "1.0.2",
		"@stripe/stripe-js": "2.2.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.33.0",
		"@tailwindcss/container-queries": "0.1.1",
		"clsx": "2.1.0",
		"dotenv": "17.2.1",
		"editorjs-html": "3.4.3",
		"libphonenumber-js": "1.10.58",
		"lodash-es": "4.17.21",
		"lucide-svelte": "^0.358.0",
		"query-string": "8.1.0",
		"svelte": "^5.38.2",
		"ts-invariant": "0.10.3",
		"url-join": "5.0.0",
		"urql": "4.0.6",
		"vite": "^5.4.19",
		"xss": "1.0.15",
		"yup": "1.3.2"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/client-preset": "4.1.0",
		"@graphql-typed-document-node/core": "3.2.0",
		"@parcel/watcher": "2.3.0",
		"@playwright/test": "1.50.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "5.3.1",
		"@sveltejs/adapter-vercel": "5.10.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.4",
		"@tailwindcss/forms": "0.5.7",
		"@tailwindcss/typography": "0.5.10",
		"@types/lodash-es": "4.17.12",
		"@types/node": "20.10.0",
		"@types/url-join": "4.0.3",
		"autoprefixer": "10.4.16",
		"dotenv-cli": "10.0.0",
		"graphql-tag": "2.12.6",
		"husky": "8.0.3",
		"is-ci": "4.1.0",
		"lint-staged": "15.1.0",
		"postcss": "8.4.32",
		"prettier": "3.1.1",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "0.5.9",
		"schema-dts": "1.1.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.3.1",
		"tailwindcss": "3.4.0",
		"typescript": "5.3.3",
		"wonka": "6.3.4"
	},
	"resolutions": {
		"graphql": "16.8.1"
	},
	"packageManager": "pnpm@10.14.0",
	"engines": {
		"pnpm": ">=9.4.0",
		"node": ">=20"
	},
	"scripts-info": {
		"package-managers": "This project supports pnpm (recommended), npm, and yarn. Use the package manager of your choice."
	}
}
