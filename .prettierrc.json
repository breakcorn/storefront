{
	"$schema": "http://json.schemastore.org/prettierrc",
	"semi": true,
	"singleQuote": false,
	"trailingComma": "all",
	"printWidth": 110,
	"useTabs": true,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"tailwindConfig": "./tailwind.config.ts",
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
